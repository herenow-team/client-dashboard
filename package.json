{
  "name": "client-dashboard",
  "version": "1.0.0",
  "description": "React boilerplate",
  "main": "index.js",
  "scripts": {
    "prettier": "prettier --single-quote --no-semi --no-bracket-spacing --trailing-comma none --write \"src/**/*.js\" --write webpack.config.js",
    "lint:js": "npm run prettier && xo --fix",
    "lint:css": "stylelint src/**/*.css --fix",
    "lint": "npm run lint:js && npm run lint:css",
    "pretest": "npm run lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "build": "cross-env NODE_ENV=development webpack --mode development",
    "build:watch": "npm run build -- -w --hide-modules",
    "build:prod": "cross-env NODE_ENV=production webpack --mode production",
    "serve": "node browsersync.js",
    "start": "run-p -r serve build:watch",
    "precommit": "lint-staged && npm test && npm run build:prod",
    "cm": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/herenow-team/client-dashboard.git"
  },
  "keywords": [
    "react",
    "boilerplate"
  ],
  "author": "Thiago Santos",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/herenow-team/client-dashboard/issues"
  },
  "homepage": "https://github.com/herenow-team/client-dashboard#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.0.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "browser-sync": "^2.24.4",
    "clean-webpack-plugin": "^0.1.19",
    "commitizen": "^2.9.6",
    "connect-history-api-fallback": "^1.5.0",
    "cross-env": "^5.1.5",
    "css-loader": "^1.0.0",
    "cssnano": "^4.0.3",
    "cz-conventional-changelog": "^2.1.0",
    "dotenv-webpack": "^1.5.5",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.0.1",
    "eslint-config-xo-react": "^0.17.0",
    "eslint-plugin-react": "^7.8.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "is-ci": "^1.1.0",
    "jest": "^22.4.3",
    "lint-staged": "^7.1.0",
    "lodash-webpack-plugin": "^0.11.5",
    "mini-css-extract-plugin": "0.4.2",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.3",
    "postcss-cssnext": "^3.1.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^2.1.5",
    "prettier": "^1.12.1",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.3.2",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "stylelint": "^9.2.1",
    "stylelint-order": "^0.8.1",
    "stylelint-suitcss": "^3.0.0",
    "svg-url-loader": "^2.3.2",
    "url-loader": "^1.0.1",
    "webpack": "^4.8.3",
    "webpack-cli": "^3.1.0",
    "xo": "^0.21.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "bulma": "^0.7.1",
    "bulma-ui": "^0.12.1",
    "classnames": "^2.2.5",
    "formik": "^1.0.1",
    "history": "^4.7.2",
    "loadable-components": "^2.1.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-google-maps": "^9.4.5",
    "react-helmet": "^5.2.0",
    "react-recaptcha": "^2.3.8",
    "react-router": "^4.2.0",
    "react-step": "^0.4.1"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "bracketSpacing": false,
    "trailingComma": "none"
  },
  "xo": {
    "parser": "babel-eslint",
    "space": true,
    "semicolon": false,
    "envs": [
      "browser",
      "jest"
    ],
    "extends": "xo-react",
    "rules": {
      "import/no-unassigned-import": 0,
      "react/jsx-tag-spacing": 0,
      "react/jsx-indent": 0,
      "react/jsx-one-expression-per-line": 0,
      "react/button-has-type": 0,
      "react/forbid-component-props": 0,
      "import/no-unresolved": 1
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:js",
      "git add"
    ],
    "*.css": [
      "npm run lint:css",
      "git add"
    ]
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./test/setup.js",
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy",
      "\\.(png|jpg|gif)$": "<rootDir>/test/__mocks__/file-mocks.js"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
